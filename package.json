{
  "name": "lnagb.js",
  "description": "Educational linear algebra environment powered by the Web",
  "type": "module",
  "main": "build/lnagb.cjs",
  "exports": {
    "require": "build/lnagb.cjs",
    "import": "build/lnagb.esm.js"
  },
  "files": [
    "build/lnagb.js",
    "build/lnagb.esm.js",
    "UNLICENSE",
    "package.json",
    "README.md",
    "src"
  ],
  "directories": {
    "lib": "lib",
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "mocha": "mocha --file test/unit/setup.cjs --require chai/register-assert --recursive",
    "test": "c8 npm run mocha",
    "build": "rollup -c build/rollup.config.js",
    "doc": "node build/doc.cjs 'src/**/*.js' 'doc'"
  },
  "c8": {
    "checkCoverage": true,
    "branches": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cgcentral/lnagb.js.git"
  },
  "keywords": [
    "linear-algebra",
    "mathematics",
    "math",
    "equations",
    "vectors",
    "matrices",
    "javascript",
    "educational"
  ],
  "author": {
    "name": "Novak",
    "email": "novakcgx@protonmail.com"
  },
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/cgcentral/lnagb.js/issues",
    "email": "novakcgx@protonmail.com"
  },
  "homepage": "https://cgcentral.github.io/lnagb.js",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "c8": "^7.7.2",
    "chai": "^4.3.4",
    "eslint": "^7.27.0",
    "eslint-config-mdcs": "^4.2.3",
    "eslint-plugin-jsdoc": "^30.7.13",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^8.4.0",
    "mocha-suppress-logs": "^0.2.0",
    "node": "^13.14.0",
    "rollup": "^2.50.5",
    "rollup-plugin-terser": "^7.0.2"
  }
}
